{
  "name": "BattleTycoon_Overworld",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Shared": {
        "$path": "src/shared"
      }
    },

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "$ignoreUnknownInstances": true,
      "Main": {
        "$path": "src/overworld/Main.server.lua"
      },
      "Services": {
        "$path": "src/overworld/Services",
        "DataService": {
          "$path": "src/server/Services/DataService.lua"
        },
        "TradeService": {
          "$path": "src/server/Services/TradeService.lua"
        },
        "BattleArenaService": {
          "$path": "src/server/Services/BattleArenaService.lua"
        },
        "MatchmakingService": {
          "$path": "src/server/Services/MatchmakingService.lua"
        },
        "GoblinCampService": {
          "$path": "src/server/Services/GoblinCampService.lua"
        },
        "ResourceNodeService": {
          "$path": "src/server/Services/ResourceNodeService.lua"
        }
      },
      "WorldBuilder": {
        "$path": "src/overworld/WorldBuilder.lua"
      },
      "MiniBaseBuilder": {
        "$path": "src/overworld/MiniBaseBuilder.lua"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",
      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "OverworldClient": {
          "$path": "src/overworld/Client"
        },
        "Controllers": {
          "$path": "src/overworld/Controllers"
        },
        "UI": {
          "$path": "src/overworld/UI"
        }
      }
    },

    "StarterGui": {
      "$className": "StarterGui",
      "UI": {
        "$path": "assets/gui",
        "$className": "Folder"
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "$properties": {
        "FilteringEnabled": true
      },
      "Terrain": {
        "$className": "Terrain"
      },
      "SpawnLocation": {
        "$className": "SpawnLocation",
        "$properties": {
          "Size": [8, 1, 8],
          "Position": [500, 0.5, 500],
          "Anchored": true,
          "Material": "Cobblestone",
          "Color": [0.471, 0.451, 0.431],
          "Neutral": true
        }
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "Technology": "Future",
        "GlobalShadows": true,
        "Brightness": 2,
        "EnvironmentDiffuseScale": 1,
        "EnvironmentSpecularScale": 1,
        "ClockTime": 14,
        "GeographicLatitude": 45
      },
      "Atmosphere": {
        "$className": "Atmosphere",
        "$properties": {
          "Density": 0.3,
          "Color": [0.9, 0.95, 1],
          "Decay": [0.85, 0.9, 1],
          "Glare": 0.1,
          "Haze": 1
        }
      }
    }
  }
}
